# -*- mode: powershell -*-

C:/Windows/system32/schtasks.exe `
  /Create /RU Administrator `
  /SC ONCE `
  /ST 00:00 `
  /SD 01/01/2099 `
  /RP setup `
  /IT `
  /TN iedriver_task `
  /TR "{{pkgPathFor "core/hab"}}\bin\hab.exe pkg exec {{pkg.ident}} pwsh {{pkg.svc_config_path}}\iedriver_task.ps1" `
  /F
